    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "nexus:generate": "ts-node --project ./tsconfig.base.json --compiler-options '{\"module\":\"CommonJS\"}' --transpile-only apps/api/src/graphql/schema",
    "api:generate": "yarn prisma:generate && yarn nexus:generate",
    "codegen:generate": "graphql-codegen --config libs/data-access/codegen.yml",
    "dependency-tree": "nx dep-graph",
    "build:all": "nx affected --target=generate-gql && yarn pwa",
    "start:all": "nx run-many --target=serve --all --parallel",
    "build:web": "nx build web-app && yarn pwa",
    "pwa": "npx workbox-cli generateSW workbox-config.js"
